{
  "name": "duncrow-gmbh/contao-jobs-bundle",
  "version": "4.1.1",
  "type": "contao-bundle",
  "homepage" : "https://github.com/duncrow-gmbh/contao-jobs-bundle",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Duncrow GmbH",
      "email": "office@duncrow.com",
      "homepage": "https://duncrow.com"
    }
  ],
  "require": {
    "terminal42/contao-changelanguage": "^3.3",
    "netzmacht/contao-font-awesome-inserttag": "^2.2",
    "numero2/contao-opengraph3": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "Duncrow\\JobsBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "files": [],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Duncrow\\JobsBundle\\ContaoManager\\Plugin"
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "contao-community-alliance/composer-plugin": true
    }
  }
}
